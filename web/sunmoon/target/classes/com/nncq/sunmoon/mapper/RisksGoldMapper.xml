<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nncq.sunmoon.dao.RisksGoldDao">


	<update id="updateRisksGold"
		parameterType="com.nncq.sunmoon.entity.RisksGold">
		UPDATE risks_gold SET
		p1=#{p1},p2=#{p2},p3=#{p3},p4=#{p4},p5=#{p5},p6=#{p6},c1=#{c1},c2=#{c2},c3=#{c3},c4=#{c4},c5=#{c5},c6=#{c6}
		where id=1
	</update>

	<select id="getRisksGold"
		resultType="com.nncq.sunmoon.entity.RisksGold">
		SELECT * FROM risks_gold WHERE id=1
	</select>


</mapper>